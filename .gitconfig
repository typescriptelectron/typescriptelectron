[core]
	repositoryformatversion = 0
	filemode = false
	bare = false
	logallrefupdates = true
	symlinks = false
	ignorecase = true
[remote "origin"]
	url = https://github.com/typescriptelectron/typescriptelectron
	fetch = +refs/heads/*:refs/remotes/origin/*
[user]
	name = typescriptelectron
	email = typescriptelectron@gmail.com
	[credential "https://github.com/typescriptelectron/typescriptelectron"]
		username = typescriptelectron
[filter "hawser"]
	clean = git hawser clean %f
	smudge = git hawser smudge %f
	required = true
